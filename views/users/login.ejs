<form class ="form-signin" action ="/users/login" method = "POST">
    <h2> Login:</h2>
    
    <label for ="txtEmail"> Email </label>
    <input type = "email" name ="email" id ="txtEmail" class = "form-control" placeholder ="Email" required autofocus>

    <label for ="txtPassword"> Password </label>
    <input type = "password" name ="password" id ="txtPassword" class = "form-control" placeholder ="Password" required autofocus>


    <button class="btn btn-lg btn-primary btn-block" type="submit">Login</button>
</form>